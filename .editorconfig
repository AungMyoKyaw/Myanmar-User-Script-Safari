# .editorconfig
# Generated for Myanmar-User-Script-Safari
root = true

[*.{js,ts,jsx,tsx,json,md,css,scss,html,htm,yml,yaml,md,markdown,txt,mdx}]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{ts,tsx,js,jsx}]
indent_size = 2
charset = utf-8

[*.json]
indent_size = 2
trim_trailing_whitespace = false

[*.md]
trim_trailing_whitespace = false
max_line_length = off

[*.yml]
indent_size = 2

[Dockerfile]
indent_style = space
indent_size = 2

[*.sh]
indent_style = space
indent_size = 2
trim_trailing_whitespace = false
insert_final_newline = true

[*.{png,jpg,jpeg,gif,webp,ico,zip,tar,gz}]
binary = true
trim_trailing_whitespace = false
insert_final_newline = false

[dist/**]
generated = true
trim_trailing_whitespace = false
insert_final_newline = false

[node_modules/**]
generated = true
trim_trailing_whitespace = false
insert_final_newline = false

[*.tsbuildinfo]
generated = true
trim_trailing_whitespace = false
insert_final_newline = false
